{
  "name": "app.js",
  "version": "1.0.0",
  "description": "Maintenance Tracker App is an application that provides users with the ability to reach out to\r operations or repairs department regarding repair or maintenance requests and monitor the\r status of their request.",
  "main": "app.js",
  "scripts": {
    "start:dev": "SET NODE_ENV=development && nodemon --watch server --exec babel-node ./server/app",
    "start:prod": "SET NODE_ENV=production && babel-node ./server/app",
    "server": "nodemon --watch server --watch client --exec babel-node ./server/app.js",
    "start:devServer": "webpack-dev-server --config webpack.dev.js --port 6000",
    "start:conc": "concurrently \" npm run server \" \"npm run start:devServer\"",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "tests": "SET NODE_ENV=test && mocha --compilers js:babel-core/register server/tests/**/*.js --timeout 10000 --exit",
    "test": "cross-env NODE_ENV=test nyc mocha server/tests/**/*.js --timeout 10000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "ui",
      "server/tests"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrkingz/Maintenance-Tracker.git"
  },
  "author": "Kingsley Frank-Demesi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrkingz/Maintenance-Tracker/issues"
  },
  "homepage": "https://github.com/mrkingz/Maintenance-Tracker#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.4.2",
    "cors": "^2.8.4",
    "cross-env": "^5.1.5",
    "css-entry-webpack-plugin": "^1.0.0-beta.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-waves": "^0.7.6",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pgtools": "^0.3.0",
    "popper": "^1.0.1",
    "popper.js": "^1.14.3",
    "proptypes": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-proptypes": "^1.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "supertest": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
