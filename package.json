{
  "name": "app.js",
  "version": "1.0.0",
  "description": "Maintenance Tracker App is an application that provides users with the ability to reach out to\r operations or repairs department regarding repair or maintenance requests and monitor the\r status of their request.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --watch server --exec babel-node ./server/app.js",
    "tests": "SET NODE_ENV=test && mocha --compilers js:babel-core/register server/test/**/*.js --timeout 20000 --exit",
    "test": "cross-env NODE_ENV=test && mocha --timeout 10000 server/test/**/*.js --compilers js:babel-core/register --exit"
  },
  "nyc": {
    "exclude": [
      "ui",
      "server/test"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrkingz/Maintenance-Tracker.git"
  },
  "author": "Kingsley Frank-Demesi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrkingz/Maintenance-Tracker/issues"
  },
  "homepage": "https://github.com/mrkingz/Maintenance-Tracker#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.5",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "supertest": "^3.1.0",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "supertest": "^3.1.0"
  }
}
